    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-webpack": "^1.7.0",
    "light-server": "^1.1.5",
    "lodash": "^4.13.1",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.11.1",
    "mocha-lcov-reporter": "^1.2.0",
    "riot": "^2.5.0",
    "riot-mui": "0.0.17",
    "riotjs-loader": "^3.0.0",
    "style-loader": "^0.13.0",
