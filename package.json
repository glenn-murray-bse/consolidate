{
  "name": "consolidate",
  "version": "1.0.0",
  "description": "Consolidation Loan Calculator",
  "author": "Glenn Murray <glenn.murray.bse@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3.10"
  },
  "scripts": {
    "deploy": "firebase deploy",
    "deploy:ci": "firebase deploy --non-interactive --token \"$FIREBASE_TOKEN\"",
    "predeploy:ci": "npm run predeploy",
    "prestart:production": "npm run clean",
    "clean": "rm -f distribution/*.js*",
    "start": "webpack-dev-server --config config/webpack/development.js --progress --inline --hot --history-api-fallback --compress --colors --https",
    "start:development": "npm start",
    "start:production": "npm run predeploy && firebase serve --debug",
    "predeploy": "webpack --config config/webpack/production.js --progress --color --compress",
    "test": "karma start karma-config.js",
    "test:circleci": "npm run test -- --single-run",
    "coverage": "open coverage/*/index.html",
    "better:coverage": "remap-istanbul -i coverage/*/coverage-final.json -o coverage/coverage-remapped.json -t json && node coverage.js && open coverage/index.html",
    "untested:dist": "webpack -p -d",
    "untested:viewdist": "light-server -s . -p 7000",
    "untested:lint": "eslint --ext .js,.es6,.html src/*"
  },
  "dependencies": {
    "firebase": "^3.2.0",
    "riot": "^2.5.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-riot": "^1.0.3",
    "babel-register": "^6.11.5",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "chrono-node": "^1.2.4",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-riot": "^0.1.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "firebase": "^3.2.0",
    "firebase-server": "^0.6.0",
    "firebase-tools": "^3.0.4",
    "ghooks": "^1.3.2",
    "istanbul": "^0.4.4",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^1.1.1",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-riot": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "light-server": "^1.1.5",
    "lodash": "^4.13.1",
    "material-design-icons": "^2.2.3",
    "material-design-lite": "^1.1.3",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.11.1",
    "mocha-lcov-reporter": "^1.2.0",
    "papaparse": "^4.1.2",
    "riot": "^2.5.0",
    "riotjs-loader": "^3.0.0",
    "style-loader": "^0.13.1",
    "uglify-loader": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
