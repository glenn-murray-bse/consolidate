{
  "name": "consolidate",
  "version": "1.0.0",
  "description": "Consolidation Loan Calculator",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack/development.js --progress --inline --hot ",
    "start:development": "npm start",
    "start:production": "npm run predeploy && firebase serve",
    "predeploy": "webpack --config config/webpack/production.js --progress",
    "deploy": "firebase deploy --only hosting",
    "test": "karma start karma-config.js",
    "test:circleci": "npm run test -- --single-run",
    "coverage": "open coverage/*/index.html",
    "better:coverage": "remap-istanbul -i coverage/*/coverage-final.json -o coverage/coverage-remapped.json -t json && node coverage.js && open coverage/index.html",
    "dev": "webpack-dev-server",
    "dist": "webpack -p -d",
    "viewdist": "light-server -s . -p 7000",
    "lint": "eslint --ext .js,.es6,.html src/*"
  },
  "author": "Glenn Murray <glenn.murray.bse@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0",
    "firebase": "^3.2.0",
    "riot": "^2.5.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-riot": "^1.0.3",
    "babel-register": "^6.11.5",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-riot": "^0.1.4",
    "firebase": "^3.2.0",
    "firebase-tools": "^3.0.4",
    "ghooks": "^1.3.2",
    "istanbul": "^0.4.4",
    "istanbul-coveralls": "^1.0.3",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^1.1.1",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-coveralls": "^1.1.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
